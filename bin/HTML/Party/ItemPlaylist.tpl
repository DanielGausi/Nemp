<table cellpadding="0" cellspacing="0">
<tbody>
<tr ID="js{{ID}}" class="{{Class}}">
	<td class="index"><span>{{Index}}</span></td>
	<td class="controls">{{BtnFileVote}}</td>
	<td class="title"><div class="title"><a href="playlist_details?id={{ID}}" title="Show more details about this title">{{Artist}}<br />{{Title}}&nbsp;<span class="duration">({{Duration}})</span><span class="{{VoteClass}}"><br /><img src="images/votesmall.png" alt="vote">&nbsp;{{Votes}} votes for this track.</span></a></div></td>
	<td class="cover"><img src="cover?id={{CoverID}}" border="0" height="50px"></td>
</tr>
</tbody>
</table>